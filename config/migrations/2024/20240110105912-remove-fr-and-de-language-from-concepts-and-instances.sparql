DELETE {
    GRAPH ?g {
        ?s ?p ?o .
    }
} WHERE {
    GRAPH ?g {
        ?s ?p ?o.
        FILTER(lang(?o) = "fr" && ?g != <http://mu.semte.ch/graphs/lpdc/ldes-data>)
    }
};

DELETE {
