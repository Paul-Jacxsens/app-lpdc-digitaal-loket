INSERT {
    GRAPH ?bestuurseenheidGraph {
        ?thombstoneInstance <http://schema.org/datePublished> ?dateDeleted .
    }
}
WHERE {
    GRAPH ?bestuurseenheidGraph {
        ?thombstoneInstance a <https://www.w3.org/ns/activitystreams#Tombstone> .
        ?thombstoneInstance <https://www.w3.org/ns/activitystreams#formerType> <http://purl.org/vocab/cpsv#PublicService> .
        ?thombstoneInstance <http://schema.org/publication> <http://lblod.data.gift/concepts/publication-status/gepubliceerd> .
        ?thombstoneInstance <https://www.w3.org/ns/activitystreams#deleted> ?dateDeleted .
    }

    FILTER NOT EXISTS {
        ?thombstoneInstance <http://schema.org/datePublished> ?datePublished .
    }

    FILTER(
    STRSTARTS(STR(?bestuurseenheidGraph), "http://mu.semte.ch/graphs/organizations/") &&
    STRENDS(STR(?bestuurseenheidGraph), "/LoketLB-LPDCGebruiker")
    )
}
